<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="server.ServerGui">
   <children>
      <TextArea fx:id="ta_server_text" layoutX="13.0" layoutY="14.0" prefHeight="481.0" prefWidth="474.0" promptText="Start by clicking the &quot;Start Server&quot; button in the down right corner." />
      <Button fx:id="btn_start_server" layoutX="408.0" layoutY="561.0" mnemonicParsing="false" onAction="#startServer" text="Start Server" />
      <Label layoutX="13.0" layoutY="503.0" text="Clients connected:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="45.0" layoutY="526.0" text="Open rooms:">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Label layoutX="10.0" layoutY="578.0" text="Server was started:" />
      <Label fx:id="lb_client_count" layoutX="143.0" layoutY="503.0" text="0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="lb_room_count" layoutX="143.0" layoutY="526.0" text="0">
         <font>
            <Font name="System Bold" size="14.0" />
         </font>
      </Label>
      <Label fx:id="lb_server_start_timestamp" layoutX="120.0" layoutY="578.0" text="Server is not running..." />
   </children>
</AnchorPane>
